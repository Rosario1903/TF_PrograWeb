package pe.edu.upc.serviceimpl;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import pe.edu.upc.entity.Oferta;
import pe.edu.upc.repository.OfertaRepository;
import pe.edu.upc.serviceinterface.IOfertaervice;

@Service
public class OfertaServiceImpl implements IOfertaervice{

	@Autowired
	private OfertaRepository cR;
	
	@Override
	public void insert(Oferta ofe) {
		try {
			cR.save(ofe);
			
		} catch (Exception e) {
			System.out.println("Error en el serviceimpl Cliente");
		}
		
	}

	@Override
	public List<Oferta> listOferta() {
		// TODO Auto-generated method stub
		return cR.findAll();
	}

	
	
}
